		<adapter class="fr.dila.reponses.api.caselink.ReponsesActionableCaseLink"
			facet="CaseLink" factory="fr.dila.reponses.core.adapter.ReponsesCaseLinkAdapterFactory" />

		<!-- *********************************************************************** 
			Adapteurs pour les feuilles de route. *********************************************************************** -->
		<adapter class="fr.dila.st.api.feuilleroute.STFeuilleRoute"
			factory="fr.dila.reponses.core.adapter.FeuilleRouteAdapterFactory" />

		<adapter class="fr.dila.reponses.api.feuilleroute.ReponsesFeuilleRoute"
			factory="fr.dila.reponses.core.adapter.FeuilleRouteAdapterFactory" />

