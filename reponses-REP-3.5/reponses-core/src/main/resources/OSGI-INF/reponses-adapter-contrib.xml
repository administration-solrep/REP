			factory="fr.dila.reponses.core.adapter.FeuilleRouteAdapterFactory" />

		<adapter class="fr.dila.reponses.api.feuilleroute.ReponsesRouteStep"
			factory="fr.dila.reponses.core.adapter.FeuilleRouteAdapterFactory" />

