			Adapteurs pour la recherche. *********************************************************************** -->
		<adapter class="fr.dila.reponses.api.recherche.Requete"
			factory="fr.dila.reponses.core.adapter.RequeteAdapterFactory" />

		<adapter class="fr.dila.reponses.api.favoris.FavoriDossier"
			factory="fr.dila.reponses.core.adapter.FavoriDossierAdapterFactory" />


