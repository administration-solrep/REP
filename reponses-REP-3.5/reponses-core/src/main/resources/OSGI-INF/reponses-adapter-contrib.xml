		<adapter class="fr.dila.reponses.api.fonddossier.FondDeDossierFolder"
			factory="fr.dila.reponses.core.adapter.FondDeDossierFolderAdapterFactory" />

		<!-- Adapteur pour la reponse -->
		<adapter class="fr.dila.reponses.api.cases.Reponse"
			factory="fr.dila.reponses.core.adapter.ReponseAdapterFactory" />

		<!-- Adapteur pour la question -->
		<adapter class="fr.dila.reponses.api.cases.Question"
			factory="fr.dila.reponses.core.adapter.QuestionAdapterFactory" />

		<!-- Adapteur DossierCommon -->
		<adapter class="fr.dila.reponses.api.cases.DossierCommon"
			factory="fr.dila.reponses.core.adapter.DossierCommonAdapterFactory" />

		<!-- *********************************************************************** 
			Adapteurs pour les jetons. *********************************************************************** -->
		<adapter class="fr.dila.st.api.jeton.JetonDoc"
			factory="fr.dila.reponses.core.adapter.JetonDocAdapterFactory" />
		<adapter class="fr.dila.reponses.api.domain.JetonDoc"
			factory="fr.dila.reponses.core.adapter.JetonDocAdapterFactory" />

		<!-- *********************************************************************** 
			Adapteurs pour les DossierLink. *********************************************************************** -->
		<adapter class="fr.dila.st.api.caselink.STDossierLink" facet="CaseLink"
			factory="fr.dila.reponses.core.adapter.DossierLinkAdapterFactory" />

		<adapter class="fr.dila.reponses.api.caselink.DossierLink"
