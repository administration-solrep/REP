<?xml version="1.0"?>

<!-- Ce composant contribue les adapteurs de Document vers les objets métiers 
	de l'application Réponses. -->
<component name="fr.dila.reponses.core.adapterContrib">
	<require>com.nuxeo.case-management.adapterContrib</require>
	<require>fr.dila.ss.core.adapterContrib</require>

	<extension target="org.nuxeo.ecm.core.api.DocumentAdapterService"
		point="adapters">

		<!-- *********************************************************************** 
			Adapteurs pour les dossiers. *********************************************************************** -->
		<adapter class="fr.dila.st.api.dossier.STDossier"
			factory="fr.dila.reponses.core.adapter.DossierAdapterFactory" />

		<adapter class="fr.dila.reponses.api.cases.Dossier" facet="Distributable"
			factory="fr.dila.reponses.core.adapter.DossierAdapterFactory" />

		<adapter class="fr.dila.reponses.api.cases.FondDeDossier" factory="fr.dila.reponses.core.adapter.FondDeDossierAdapterFactory" />

		<adapter class="fr.dila.reponses.api.fonddossier.FondDeDossierFile"
