## DO NOT EDIT THIS FILE, USE nuxeo.conf  ##
nuxeo.template.includes=cmf-reponses,ldap-local

# publicly visible URL
nuxeo.url=http://localhost:8080/reponses
# internally visible URL (server-to-server)
nuxeo.loopback.url=http://localhost:8080/reponses

# Application context path
org.nuxeo.ecm.contextPath=/reponses


nuxeo.server.http.port=8080
nuxeo.server.ajp.port=8009
nuxeo.server.https.port=8443
nuxeo.server.tomcat-admin.port=8005

org.nuxeo.ecm.instance.name=Nuxeo 5.4
org.nuxeo.ecm.instance.description=Nuxeo ECM server

# Mail server protocol
mail.store.protocol=pop3
mail.transport.protocol=smtp
# Use TLS for SMTP connections
mail.smtp.usetls=false
# User who will receive mail
mail.user=nobody
# Mail server
mail.pop3.host=pop3.nosuchhost.nosuchdomain.com
# Enable debugging output from the javamail classes
mail.debug=false

nuxeo.bind.address=0.0.0.0

nuxeo.db.url=jdbc:h2:nuxeo;create=true;MVCC=true
nuxeo.db.name=
nuxeo.db.user=
nuxeo.db.password=
nuxeo.db.driver=org.h2.Driver
nuxeo.db.classpath=

solrep.db.liquibase.contexts=dev,reponses
