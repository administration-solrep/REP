                <qu:rubrique_ta>professeurs documentalistes</qu:rubrique_ta>
                <qu:analyse>rémunérations</qu:analyse>
            </qu:indexation_an>
        </qu:question>
    </wsq:questionReponse>
</wsq:envoyerQuestionsRequest>
]]></con:request>
