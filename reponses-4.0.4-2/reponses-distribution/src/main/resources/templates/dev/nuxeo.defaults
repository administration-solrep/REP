## DO NOT EDIT THIS FILE, USE nuxeo.conf  ##
dev.target=.

# to use https, add template 'https' and configure accordingly
#nuxeo.server.https.port=8443

org.nuxeo.dev=true

# Mail server protocol
mail.store.protocol=pop3
mail.transport.protocol=smtp
# Use TLS for SMTP connections
mail.transport.usetls=false
# User who will receive mail
mail.user=nobody
# Mail server
mail.store.host=pop3.nosuchhost.nosuchdomain.com
# Enable debugging output from the javamail classes
mail.debug=false

nuxeo.bind.address=0.0.0.0

